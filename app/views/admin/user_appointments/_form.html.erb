<div class="control-group">
   <!-- <label class="control-label" for="input1">Adı</label> -->
   <div class="controls">
      <%= f.input :first_name, input_html: { class: "span6", id: "input1"}, label: "Adı" %>
   </div>
</div>
<div class="control-group">
   <!-- <label class="control-label" for="input1">Adı</label> -->
   <div class="controls">
      <%= f.input :last_name, input_html: { class: "span6", id: "input2"}, label: "Soyadı" %>
   </div>
</div>
<div class="control-group">
   <!-- <label class="control-label" for="input1">Adı</label> -->
   <div class="controls">
      <%= f.input :email, input_html: { class: "span6", id: "input3"}, label: "E-Posta" %>
   </div>
</div>
<div class="control-group">
   <!-- <label class="control-label" for="input1">Adı</label> -->
   <div class="controls">
    <%= f.input :address, input_html: { class: "span6", id: "inputRemarks", rows: "3"}, as: :text, label: "Adres" %>
   </div>
</div>
<div class="control-group">
   <!-- <label class="control-label" for="input1">Adı</label> -->
   <div class="controls">
      <%= f.input :phone_number, input_html: { class: "span6", id: "input4"}, label: "Telefon Numarası" %>
   </div>
</div>

<div class="control-group">
   <!-- <label class="control-label" for="input1">Adı</label> -->
   <div class="controls">
    <%= f.association :brand, label_method: :brand_name, value_method: :id, input_html: { class: "span6", id: "input5"}, label: "Marka Adı" %>
   </div>
</div>
<div class="control-group">
   <!-- <label class="control-label" for="input1">Adı</label> -->
   <div class="controls">
      <%= f.association :brand_make ,label_method: :make_name, value_method: :id, input_html: { class: "span6", id: "input6"}, label: "Model Adı" %>
   </div>
</div>
<div class="control-group">
   <!-- <label class="control-label" for="input1">Adı</label> -->
   <div class="controls">
    <%= f.input :care_operation, input_html: { class: "span6", id: "inputRemarks", rows: "3"}, as: :text, label: "Adres" %>
   </div>
</div>



<div class="form-actions">
  <%= button_tag(type: 'submit', class: "btn btn-primary") do %>
     Kaydet
  <% end %>
   
   
   <!-- <button type="button" class="btn">Cancel</button> -->
</div>

